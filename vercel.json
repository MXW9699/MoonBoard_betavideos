{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "client",
      "use": "@vercel/static-build",
      "config": { "outputDirectory": "build" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/login",
      "dest": "/server/server.js"
    },
    {
      "src": "/data",
      "dest": "/server/server.js"
    },
    {
      "src": "/profile",
      "dest": "/server/server.js"
    },
    {
      "src": "/updateHolds",
      "dest": "/server/server.js"
    },
    {
      "src": "/problemList",
      "dest": "/server/server.js"
    },
    {
      "src": "/video",
      "dest": "/server/server.js"
    }
  ]
}
